{
  "name": "link_shortener_backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "serve-dev": "nodemon ./bin/www",
    "add-migration": "node ./node_modules/sequelize-cli/lib/sequelize --options-path ./.sequelizerc migration:create --name",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize --options-path ./.sequelizerc db:migrate",
    "revert": "node ./node_modules/sequelize-cli/lib/sequelize --options-path ./.sequelizerc db:migrate:undo"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.9",
    "base62": "^2.0.1",
    "body-parser": "^1.20.1",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "klaw-sync": "^6.0.0",
    "morgan": "~1.9.1",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.12"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1"
  }
}
